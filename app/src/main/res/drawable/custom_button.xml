<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <gradient
                android:startColor="#005D6B"
                android:endColor="#005D6B"
                android:angle="270" />
            <stroke
                android:width="0dp"
                android:color="#005D6B" />
            <corners
                android:radius="3dp" />
            <padding
                android:left="20dp"
                android:top="20dp"
                android:right="20dp"
                android:bottom="20dp" />
        </shape>
    </item>

    <item android:state_enabled="true" >
        <shape>
            <gradient
                android:endColor="#434154"
                android:startColor="#434154"
                android:angle="270" />
            <stroke
                android:width="3dp"
                android:color="#434154" />
            <corners
                android:radius="3dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>

